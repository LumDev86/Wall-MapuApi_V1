# API Configuration
EXPO_PUBLIC_API_URL=https://wall-mapuapi-production.up.railway.app/api

# App Configuration
EXPO_PUBLIC_APP_NAME=Wallmapu
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment (development | staging | production)
EXPO_PUBLIC_ENV=production

# Google Maps API Key (required for map features)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Supabase Configuration (required for image uploads)
# Images are uploaded directly to Supabase Storage from the frontend
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Note: Make sure your Supabase bucket "Wall-MapuApi" is configured as public
# and has the following folder structure:
#   - shops/logos/{shopId}/
#   - shops/banners/{shopId}/
#   - products/{shopId}/{productId}/
#   - categories/{categoryId}/
