# API Configuration
EXPO_PUBLIC_API_URL=https://your-api-url.com/api

# App Configuration
EXPO_PUBLIC_APP_NAME=Wallmapu
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment (development | staging | production)
EXPO_PUBLIC_ENV=development

# Google Maps API Key (required for map features)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
# IMPORTANT: Restrict this key to your app's bundle ID in Google Cloud Console
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Supabase Configuration (required for image uploads)
# Get these values from: https://app.supabase.com/project/_/settings/api
# Images are uploaded directly to Supabase Storage from the frontend
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
